{
  "name": "@eth-patex/sdk",
  "version": "2.0.1",
  "description": "[Patex] Tools for working with Patex",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "all": "yarn clean && yarn build && yarn test && yarn lint:fix && yarn lint",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist/ ./tsconfig.tsbuildinfo",
    "lint": "yarn lint:fix && yarn lint:check",
    "lint:check": "eslint . --max-warnings=0",
    "lint:fix": "yarn lint:check --fix",
    "pre-commit": "lint-staged",
    "test": "hardhat test",
    "test:coverage": "nyc hardhat test && nyc merge .nyc_output coverage.json",
    "autogen:docs": "typedoc --out docs src/index.ts"
  },
  "keywords": [
    "patex",
    "ethereum",
    "sdk"
  ],
  "homepage": "https://github.com/patex-ecosystem/sdk#readme",
  "license": "MIT",
  "author": "Patex",
  "repository": {
    "type": "git",
    "url": "https://github.com/patex-ecosystem/sdk.git"
  },
  "devDependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/node": "^20.2.5",
    "chai-as-promised": "^7.1.1",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.7.0",
    "hardhat": "^2.9.6",
    "hardhat-deploy": "^0.11.4",
    "typedoc": "^0.22.13",
    "ts-node": "^10.9.1",
    "@babel/eslint-parser": "^7.18.2",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.3.0",
    "depcheck": "^1.4.3",
    "doctoc": "^2.2.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^6.0.0",
    "lint-staged": "11.0.0",
    "markdownlint": "^0.24.0",
    "markdownlint-cli2": "0.4.0",
    "mkdirp": "^1.0.4",
    "nyc": "^15.1.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.8.0",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.3",
    "mocha": "^10.0.0"
  },
  "dependencies": {
    "@eth-patex/contracts": "^1.0.0",
    "@eth-patex/contracts-bedrock": "^1.0.0",
    "@eth-patex/core-utils": "^1.0.0",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.2.27",
    "rlp": "^2.2.7"
  },
  "peerDependencies": {
    "ethers": "^5"
  }
